{
  "expo": {
    "name": "BORA Aluno",
    "slug": "bora",
    "version": "1.0.0",
    "sdkVersion": "54.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#00C853"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bora.aluno"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#00C853"
      },
      "package": "com.bora.aluno"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "bora-aluno",
    "jsEngine": "hermes",
    "updates": {
      "enabled": false,
      "checkAutomatically": "NEVER",
      "fallbackToCacheTimeout": 0
    },
    "plugins": [
      "expo-dev-client",
      "expo-router",
      "expo-secure-store",
      "expo-location",
      "expo-asset",
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#00C853"
        }
      ],
      "@react-native-community/datetimepicker",
      [
        "expo-build-properties",
        {
          "android": {
            "kotlinVersion": "2.0.0"
          }
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "4844f00e-55a6-4a17-abbc-9a6ba516ee49"
      }
    },
    "owner": "boradevvs-organization"
  }
}